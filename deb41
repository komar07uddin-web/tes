    Legendary = {"Lined Cardinal Fish", "Yellowfish Tuna", "Chrome Tuna", "Bumblebee Grouper", "Lobster", "Lavafin Tuna", "Starjam Tang", "Greenbee Grouper", "Blue Lobster", "Enchanted Angelfish", "Magic Tang", "Axolotl", "Deep Sea Crab"},
