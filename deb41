-- FIND MINIGAME ANIMATION NAME
DebugSection:NewButton("ğŸ” Scan Minigame Anim", "Find the animation name", function()
    task.spawn(function()
        StarterGui:SetCore("SendNotification", {Title = "ğŸ” Scanning...", Text = "Equip fishing rod then start fishing", Duration = 5})
        task.wait(3) -- Give the player time to equip the rod and start fishing
        local playingAnimations = getAllPlayingAnimationNames()
        if #playingAnimations > 0 then
            -- Print all animation names for debugging.  Leave this in for easier maintenance.
            logInfo("FISHING", "Animations playing: " .. table.concat(playingAnimations, ", ")) -- Ask the user to choose which animation is the correct minigame animation
        local chosenAnimation = Library.AcceptInput("Choose Animation", "Which animation is the minigame?", "")
        if chosenAnimation and chosenAnimation ~= "" then
            state.minigameAnimationName = chosenAnimation
            logInfo("FISHING", "Minigame animation name set to: " .. chosenAnimation)
            StarterGui:SetCore("SendNotification", {Title = "âœ… Animation Found!", Text = "Minigame animation: " .. chosenAnimation, Duration = 5})
        else
            logWarn("FISHING", "âš ï¸ No animation chosen.")
            StarterGui:SetCore("SendNotification", {Title = "âš ï¸ No Animation", Text = "No animation chosen.", Duration = 5})
        end
    else
        logWarn("FISHING", "âš ï¸ No animations are playing.");
        StarterGui:SetCore("SendNotification", {Title = "âš ï¸ No Animations", Text = "No animations are playing.", Duration = 5})
    end
end)end)

-- FISH CAUGHT EVENT
if RE_FishCaught then
    pcall(function()
        RE_FishCaught.OnClientEvent:Connect(function(fishData)
            if fishData and fishData.Name then
                local fishName = tostring(fishData.Name)
                logInfo("CATCH", "ğŸ£", fishName)
                local tier = getFishTier(fishName)
                if tier then alertRareCatch(fishName, tier) end
            end
        end)
        logInfo("EVENT", "âœ… Fish event registered")
    end)
else
    logWarn("EVENT", "âš ï¸ Fish event not found")
end

