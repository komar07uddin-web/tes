            end
        })
    end)
    
    if not dropdownSuccess then
        logWarn("GUI", "Dropdown failed, using buttons")
        Tabs["Fishing"]:Label("Select Timing Mode:")
        for _, mode in ipairs({"Perfect", "Amazing", "Good", "Safe"}) do
            pcall(function()
