-- üóÇÔ∏è Create Tabs
local Tabs = {}
local function createTab(name, color, icon)
    local ok, tab = pcall(function()
        return Main:Tab({ Name = name, tabColor = Color3.fromHex(color), Image = icon })
    end)
    if ok and tab then
        Tabs[name] = tab
        logInfo("GUI", "‚úÖ Tab:", name)
    else
        logError("GUI", "‚ùå Tab failed:", name)
    end
end

