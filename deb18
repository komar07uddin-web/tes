local function teleportTo(position, name)
    perfStart("Teleport")
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        pcall(function()
            player.Character.HumanoidRootPart.CFrame = CFrame.new(position)
            logInfo("TELEPORT", "âœ… Teleported to:", name)
        end)
    end
    perfEnd("Teleport")
end

-- ========== TELEPORT TO PLAYER (FIXED) ==========
local function teleportToPlayer(username)
    logInfo("TELEPORT", "ğŸš€ Attempting teleport to player:", username)
    
    if not username or username == "" then
        logError("TELEPORT", "âŒ Username is empty")
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "âš ï¸ Teleport Failed",
            Text = "Please enter a username first!",
            Duration = 3
        })
        return
    end
    
    logDebug("TELEPORT", "Searching for player:", username)
    
    local targetPlayer = nil
    
    -- Try exact match first
    targetPlayer = game.Players:FindFirstChild(username)
    
    -- If not found, try case-insensitive search
    if not targetPlayer then
        logDebug("TELEPORT", "Exact match not found, trying case-insensitive...")
        for _, player in ipairs(game.Players:GetPlayers()) do
            if string.lower(player.Name) == string.lower(username) then
                targetPlayer = player
                logDebug("TELEPORT", "Found via case-insensitive:", player.Name)
                break
            end
        end
    end
    
    -- If still not found, try partial match
    if not targetPlayer then
        logDebug("TELEPORT", "Case-insensitive not found, trying partial match...")
        for _, player in ipairs(game.Players:GetPlayers()) do
            if string.find(string.lower(player.Name), string.lower(username)) then
                targetPlayer = player
                logDebug("TELEPORT", "Found via partial match:", player.Name)
                break
            end
        end
    end
    
    if not targetPlayer then
        logError("TELEPORT", "âŒ Player not found:", username)
        logInfo("TELEPORT", "Available players:")
        for _, player in ipairs(game.Players:GetPlayers()) do
            logInfo("TELEPORT", " -", player.Name)
        end
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "âŒ Player Not Found",
            Text = username .. " is not in this server!",
            Duration = 3
        })
        return
    end
    
    logInfo("TELEPORT", "âœ… Player found:", targetPlayer.Name)
    
    if not targetPlayer.Character then
        logError("TELEPORT", "âŒ Player has no character:", targetPlayer.Name)
        return
    end
    
    local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not targetHRP then
        logError("TELEPORT", "âŒ Cannot find HumanoidRootPart for:", targetPlayer.Name)
        return
    end
    
    local player = game.Players.LocalPlayer
    if not player or not player.Character then
        logError("TELEPORT", "âŒ Local player character not found")
        return
    end
    
    local localHRP = player.Character:FindFirstChild("HumanoidRootPart")
    if not localHRP then
        logError("TELEPORT", "âŒ Local HumanoidRootPart not found")
        return
    end
    
    local success = pcall(function()
        localHRP.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 3)
        logInfo("TELEPORT", "âœ… Teleported to player:", targetPlayer.Name)
    end)
    
    if success then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "âœ… Teleport Success",
            Text = "Teleported to " .. targetPlayer.Name,
            Duration = 3
        })
    else
        logError("TELEPORT", "âŒ Teleport execution failed")
    end
end

-- ========== ANTI-AFK SYSTEM ==========
local function startAntiAFK()
    task.spawn(function()
        while state.antiAFK do
            local player = game.Players.LocalPlayer
            if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local hrp = player.Character.HumanoidRootPart
                local randomOffset = Vector3.new(math.random(-2, 2), 0, math.random(-2, 2))
                hrp.CFrame = hrp.CFrame + randomOffset
            end
            task.wait(math.random(45, 90))
        end
    end)
    logInfo("STABILITY", "âœ… Anti-AFK enabled")
end

-- ========== EMERGENCY RESET ==========
local function emergencyReset()
    logWarn("RESET", "ğŸš¨ EMERGENCY RESET TRIGGERED")
    
    state.autoFishing = false
    state.autoSell = false
    state.antiAFK = false
    state.enableSpoof = false
    
    task.wait(0.5)
    
    stats.fishCaught = 0
    stats.rareCaught = 0
    stats.totalCycles = 0
    stats.rareFish = {}
    
    logInfo("RESET", "âœ… All systems stopped and stats cleared")
end

