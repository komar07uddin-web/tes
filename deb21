-- ========== STATE ==========
local state = {
    autoFishing = false,
    semiMode = false,
    tapSpeed = 100, -- milliseconds (0-500)
    fishingDelay = {
        equip = 0.3,
        charge = 0.5,
        cast = 1.0,
        bite = 4.0,
        catch = 0.5,
        rest = 2.0
    },
